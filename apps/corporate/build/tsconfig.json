{
  "compilerOptions": {
      "module": "commonjs",
      "esModuleInterop": true,
      "target": "es6",
      "noImplicitAny": true,
      "strictNullChecks": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "sourceMap": true,
      "outDir": "../.tmp-build",
      "baseUrl": "./build",
      "paths": {
          "*": [
              "node_modules/*"
          ]
      },
    "typeRoots": [
      "node_modules/@types",
    ],
    "types": ["node"],
  }
}
