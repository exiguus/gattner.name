{
  "name": "@gattner/storybook",
  "version": "1.0.0",
  "description": "Storybook components, config, parameters and decorators",
  "main": "dist/cjs/src/index.js",
  "module": "dist/es/src/index.js",
  "types": "dist/types/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/src/index.d.ts",
      "module": "./dist/es/src/index.js",
      "require": "./dist/cjs/src/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rm -rf ./dist/ && pnpm build:es && pnpm build:cjs",
    "build:es": "NODE_ENV=production tsc",
    "build:cjs": "NODE_ENV=production tsc --project tsconfig.commonjs.json",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,mjs,html}\"",
    "lint": "eslint . --ext ts --ext tsx --ext js --ext jsx --ext mjs",
    "test:unit": "dotenv -e .env.local -- jest"
  },
  "keywords": [
    "Storybook",
    "DocsPage"
  ],
  "author": {
    "name": "Simon Gattner",
    "url": "https://gattner.name/",
    "twitter": "https://twitter.com/exiguus_"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/exiguus/gattner.name.git/packages/storybook"
  },
  "homepage": "https://github.com/exiguus/gattner.name/packages/storybook#readme",
  "devDependencies": {
    "@babel/core": ">=7.13.0 <8.0.0",
    "@babel/plugin-syntax-flow": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@jest/globals": "^29.0.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.18",
    "@types/react": "16",
    "@types/react-dom": "16",
    "@types/styled-components": "5",
    "@types/testing-library__jest-dom": "^5.14.5",
    "dotenv": "^16.0.2",
    "dotenv-cli": "^6.0.0",
    "jest": "^29.0.3",
    "jest-watch-typeahead": "^2.2.0",
    "ts-jest": "^29.0.1",
    "typescript": "^4.8.3",
    "webpack": "5"
  },
  "dependencies": {
    "@gattner/ui-theme": "workspace:^1.0.0",
    "@storybook/addon-docs": "^6.5.12",
    "@storybook/addons": "^6.5.12",
    "@storybook/react": "^6.1.17",
    "react": "16",
    "react-dom": "16",
    "react-markdown": "^8.0.3",
    "remark-gfm": "^3.0.1",
    "require-from-string": "^2.0.2"
  },
  "peerDependencies": {
    "@storybook/addon-a11y": "^6.5.12",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-backgrounds": "^6.5.12",
    "@storybook/addon-controls": "^6.5.12",
    "@storybook/addon-docs": "^6.5.12",
    "@storybook/addon-essentials": "^6.1.17",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/addon-viewport": "^6.5.12",
    "@storybook/addons": "^6.5.12",
    "@storybook/node-logger": "^6.1.17",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.1.17"
  }
}
