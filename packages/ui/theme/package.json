{
  "name": "@gattner/ui-theme",
  "version": "1.0.0",
  "description": "Theme in Shared UI Library",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "module": "./dist/es/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rm -rf ./dist/ && pnpm build:es && pnpm build:cjs && cp -R ./assets ./dist/",
    "build:es": "NODE_ENV=production tsc",
    "build:cjs": "NODE_ENV=production tsc --project tsconfig.commonjs.json",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,mjs,html}\"",
    "lint": "eslint . --ext ts --ext tsx --ext js --ext jsx --ext mjs",
    "test:unit": "dotenv -e .env.local -- jest"
  },
  "keywords": [
    "Theme",
    "Fonts",
    "Global Styles"
  ],
  "author": {
    "name": "Simon Gattner",
    "url": "https://gattner.name/",
    "twitter": "https://twitter.com/exiguus_"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/exiguus/gattner.name.git/packages/ui/theme"
  },
  "homepage": "https://github.com/exiguus/gattner.name/packages/ui/theme#readme",
  "devDependencies": {
    "@gattner/types": "workspace:^1.0.0",
    "@jest/globals": "^29.0.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.18",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.25",
    "@types/testing-library__jest-dom": "^5.14.5",
    "dotenv": "^16.0.2",
    "dotenv-cli": "^6.0.0",
    "jest": "^29.0.3",
    "jest-watch-typeahead": "^2.2.0",
    "ts-jest": "^29.0.1",
    "typescript": "^4.8.3"
  },
  "peerDependencies": {
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-is": "^16.13.1",
    "styled-components": "^5.3.11"
  },
  "dependencies": {
    "@gattner/utils": "workspace:^1.0.0"
  }
}